! Xft settings ---------------------------------------------------------------
Xft.dpi:        96
Xft.antialias:  true
Xft.hinting:    true
Xft.hintstyle:  hintslight
Xft.rgba:       rgb

! urxvt settings -------------------------------------------------------------
URxvt.letterSpace: -1
URxvt.font: xft:PragmataPro Mono:pixelsize=13:hinting=true
URxvt.perl-ext-common: default,matcher
URxvt.urlLauncher: firefox
URxvt.matcher.button: 1
URxvt.scrollBar: off

! xoria256: http://snk.tuxfamily.org/log/xoria256-terminal-color-scheme.html
URxvt.color0      : #121212
URxvt.color8      : #0a0a0a
URxvt.color1      : #d78787
